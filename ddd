for(int i = 0 ; i < count ; i ++){
        temp = rand() % 45 + 1;
        if(i == 0){
            arr[i] = temp;
        }
        else{
            int j=0;
            while(j < i){
                if(arr[j] - temp != 0){
                    j++;
                }
                else{
                    temp = rand() % 45 + 1;
                    }
            }
        }
    }

    for(int i = 0 ; i < count ; i++){
        cout << i << "number is :" << arr[i] << endl;
    }